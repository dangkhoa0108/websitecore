@model websitecore.ViewModel.BrandViewModel
@{
    Layout = null;
}

<div class="brands-area">
    <div class="zigzag-bottom"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="brand-wrapper">
                    <div class="brand-list">
                        @foreach (var item in Model.lstBrand)
                        {
                            <img src="@item.BrandImage" alt="">
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div> <!-- End brands area -->